    "ora": "5.4.1",
    "semver": "7.6.3",
    "yaml": "2.6.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.30.1",
    "eslint": "9.30.1",
