{
  "name": "cloudycs-repo-manager",
  "version": "1.0.0",
  "description": "A comprehensive repository manager for creating project structures across different programming languages and frameworks",
  "main": "src/index.js",
  "bin": {
    "repo-manager": "./bin/repo-manager.js",
    "rm": "./bin/repo-manager.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "build": "npm run build:templates",
    "build:templates": "node scripts/build-templates.js",
    "test": "jest",
    "dev": "nodemon src/index.js",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "keywords": [
    "repository",
    "template",
    "scaffold",
    "generator",
    "aws-cdk",
    "terraform",
    "devops",
    "project-structure"
  ],
  "author": "CloudyCS",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "inquirer": "^9.2.0",
    "chalk": "^4.1.2",
    "fs-extra": "^11.1.1",
    "mustache": "^4.2.0",
    "yaml": "^2.3.0",
    "semver": "^7.5.0",
    "ora": "^5.4.1",
    "boxen": "^5.1.2"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "nodemon": "^3.0.0",
    "eslint": "^8.44.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
