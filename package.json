{
  "name": "cloudycs-repo-manager",
  "version": "1.0.0",
  "description": "A comprehensive repository manager for creating project structures across different programming languages and frameworks",
  "main": "src/index.js",
  "bin": {
    "repo-manager": "./bin/repo-manager.js",
    "rm": "./bin/repo-manager.js"
